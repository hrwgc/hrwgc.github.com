			function selectsubjectFilters(obj,evtType){
				var selectedData = $(obj).val();
				var multiSelectLength;
				var multiSelectoptionLen;
				var selectedCategoryData = $('#Level_of_Study').val()
				var url = '/bc?c=Page&cid=1262438028287&pagename=GLOBAL%2FSubjectListingTemplate';//+'?seledtedData='+seledtedData;
