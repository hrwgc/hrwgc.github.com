				  //$('.clearFilter').trigger('click');
				
				}
				
			jQuery.ajax({
			url: url, 
			type: 'POST',
			data: {"selectedData":selectedData,"selectedCategoryData":selectedCategoryData},
			success: function(html){
				if(evtType=='load'){
					multiSelectLength = $(html).find("option").filter(':selected').length;
					multiSelectoptionLen = $(html).find("option").length;
				}
				$('#course_navtabs #subjectMapping').empty().html(html);
     			$("#course_navtabs #subjectMapping .jsMultiDropdown").multiSelect();
				if(evtType=='load' && multiSelectLength>0){
					$("#course_navtabs #studylevel1").find('span').text(multiSelectLength+' selected');
					if(multiSelectLength==multiSelectoptionLen){
						$('#course_navtabs .multiSelectOptions input.selectAll').attr('checked','true')
						$('#course_navtabs .multiSelectOptions input.selectAll').parent().addClass('checked');
						}
					}
				$("#course_navtabs #subjectSelect").css('display','block');


			},
			error: function(){}
		});
		
		$("#course_navtabs #subjectSelect").css("display","block");
