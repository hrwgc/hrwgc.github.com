<script type="text/javascript">

// Set flag below to:
//
//  login    - regular login page     (file -> login.htm)
//  loginbu  - regular error page     (file -> loginbu.htm)
//  loginsu  - shibboleth error page  (file -> loginsu.htm)

var flag="login";

// Generate 'div' with style info
if (flag == 'loginbu' || flag == 'loginsu') { document.write('<div style="padding: 5px;font-family: Verdana, Arial, Helvetica, sans-serif;font-weight: bold;font-size: 8pt;text-align: center;padding: 5px;color: red;border: solid thin red;">'); }

// Regular Error Message
if (flag == 'loginbu')   { document.write('ERROR:&nbsp;Your <i>Library Login</i> attempt has failed! Please check your barcode and PIN and try again!  <br/>For assistance, contact the UBC Library Help Desk at 604-822-6275 or library.systems@ubc.ca.'); }

// Shibboleth Error Message
if (flag == 'loginsu')    { document.write('ERROR:&nbsp;Your <i>CWL Login</i> attempt has failed! To login, you must have an active affiliation with UBC (faculty, staff, student, etc) and a valid UBC Campus Wide Login account.  Please try again!  <br/>For assistance, contact the UBC Library Help Desk at 604-822-6275 or library.systems@ubc.ca.'); }

if (flag == 'loginbu' || flag == 'loginsu') { document.write('</div>'); }

</script>

